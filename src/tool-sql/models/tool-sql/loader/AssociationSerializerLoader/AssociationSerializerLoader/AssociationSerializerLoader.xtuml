INSERT INTO O_OBJ
	VALUES ("bb4bd1f7-e46b-46a4-a0c2-c8aa4c8af654",
	'AssociationSerializerLoader',
	1,
	'AssociationSerializerLoader',
	'Instances are configured to render methods for the persistent associations defined in the compiled application.
Instances hold values to be supplied to templates rendering code for serialization and reinstatement of associations.',
	"00000000-0000-0000-0000-000000000000");
INSERT INTO O_TFR
	VALUES ("65d6dce4-0e44-4b26-924e-c69833038ea5",
	"bb4bd1f7-e46b-46a4-a0c2-c8aa4c8af654",
	'render_invocation',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'// Render invocation of serialization method(s) for an association.

rnum_str =  STRING::itoa( i: self.rel_num );

select one srefferer related by self->SingleReferrerAssoc[R3029];
if ( not_empty srefferer )
  class_name = srefferer.referring_class_name;
  T::include(file:"loader/t.linkserializer.invoke.java");
else
  // subtypes each need a serialization method.
  select one mrefferer related by self->MultipleReferrerAssoc[R3029];
  select many referrers related by mrefferer->ReferringClassProxy[R3036.''holds referring data for''];
  for each referrer in referrers
    class_name = referrer.referring_class_name;
    T::include(file:"loader/t.linkserializer.invoke.java");
  end for;
end if;',
	1,
	'',
	"228f4eac-3830-4129-8f85-5b89ce7520c5",
	0,
	0);
INSERT INTO O_TFR
	VALUES ("240626d1-97c2-466d-be12-619fc39ebbfd",
	"bb4bd1f7-e46b-46a4-a0c2-c8aa4c8af654",
	'render_serialize',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'// Taking the place of ''deferred'' operation, pending fix for package references. See OneFact issue SI #12165

select one singleref related by self->SingleReferrerAssoc[R3029];
if ( not_empty singleref )
  singleref.render_serialize();
else
  select one multiref related by self->MultipleReferrerAssoc[R3029];
  multiref.render_serialize();
end if;',
	1,
	'',
	"65d6dce4-0e44-4b26-924e-c69833038ea5",
	0,
	2);
INSERT INTO O_TFR
	VALUES ("228f4eac-3830-4129-8f85-5b89ce7520c5",
	"bb4bd1f7-e46b-46a4-a0c2-c8aa4c8af654",
	'create',
	'',
	"53b58fe9-fed0-4bfd-aad6-5c3803878784",
	0,
	'// Create a serializer instance and attach it to the population serializer/loader instance for this component. 
create object instance assoc_serializer of AssociationSerializerLoader;
assoc_serializer.rel_num = param.rel_num;
loader = param.addto;
relate assoc_serializer to loader across R3019.''renders for'';
return assoc_serializer;',
	1,
	'',
	"00000000-0000-0000-0000-000000000000",
	0,
	3);
